{
  "name": "artik-proxy-hub",
  "version": "1.4.1-beta",
  "private": true,
  "main": "index.js",
  "scripts": {
    "postinstall": "find ./proxies/ -maxdepth 2 -name package.json -execdir npm install \\;",
    "installui": "npm install --prefix ui/; npm run buildui;",
    "test": "find tests/*-tests.js -exec node {} \\;",
    "clean": "rm data/*.json",
    "webpack": "cd web-ui/; npm install; npm run webpack; cd ..",
    "start": "node index.js config.json",
    "start:ui": "npm run buildui; npm start",
    "start:devui": "npm run devui; npm start",
    "devui": "npm run dev --prefix ui/",
    "buildui": "npm run build --prefix ui/",
    "watchui": "npm run watch --prefix ui/",
    "release": "./release.sh"
  },
  "dependencies": {
    "artikcloud-js": "4.0.0",
    "body-parser": "^1.15.2",
    "chokidar": "^1.7.0",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "ip": "^1.1.3",
    "node-ssdp": "^2.7.2",
    "passport": "^0.3.2",
    "passport-oauth": "^1.0.0",
    "promise": "^7.1.1",
    "request": "^2.81.0",
    "session-file-store": "^0.2.0",
    "socket.io": "^1.4.8",
    "winston": "^2.2.0",
    "winston-logrotate": "^1.3.0",
    "ws": "^1.1.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "eslint": "^4.4.1",
    "supertest": "^1.2.0",
    "tape": "^4.6.0"
  }
}
